

 <table border="1" class="mg-showdata-bg mg-tbl-layout" cellspacing="1" cellpadding="1">
   <tr>
     <td class="mg-td-width-half" valign="top">
   		<label class="mg-label"> <%= t('name')%></label>
   	 </td>
   	 <td class="mg-td-width-half" valign="top">
  		<%if @document.name.present?%>
     <%=@document.name%>
     <%else%>
     <%="No Data"%>
     <%end%>
  	 </td>
 </tr>


<tr>
     <td class="mg-td-width-half" valign="top">
      <label class="mg-label"> <%= t('extention')%></label>
     </td>
     <td class="mg-td-width-half" valign="top">
      <%if @document.extention.present?%>
     <%=@document.extention%>
     <%else%>
     <%="No Data"%>
     <%end%>
     </td>
 </tr>


 <tr>
     <td class="mg-td-width-half" valign="top">
      <label class="mg-label"> <%= t('description')%></label>
     </td>
     <td class="mg-td-width-half" valign="top" width="120">
      <%if @document.description.present?%>
     <%=@document.description%>
     <%else%>
     <%="No Data"%>
     <%end%>
