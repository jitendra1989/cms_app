<!-- Sec --> 
<div id="page-yield">
 	<div class="box">
 		<%= form_for :generate_fine_form, :url=>{:controller=>'fees',:id=>@generate_fine_form.id,    :action=>'update_fee_fine'}, method: :patch,:html => { :id => "latefeeFineEditFormId",:multipart => true} do |f| %>
 			
 			<%= f.hidden_field :is_deleted, :value=> 0%>
 			<%= f.hidden_field :mg_school_id, :value=> session[:current_user_school_id]%>

 			<div class="generate_fine_form-content">
          		<div class="generate_fine_form-inner">
            		<table cellspacing="0" cellpadding="0" border="0" class="generate_fine_table-cls mg-table-spacing">
              			<tbody>

              				<tr class="fine-generate-tr-cls">
			                  	<td style="width:220px;" class="fee-fine-td" >
			                  		<label for="fine_name" class="mg-label"><%= t('FineName') %><span class="required-field">&nbsp;*</span></label>
			                  	</td>
			                  	<td class="fee-fine-td">
			                    	<%= f.text_field :fine_name ,:class=>"mg-text-field"%>
			                  	</td>
		                  	</tr>

		                  	<tr>
			                  	<td valign="top">
			                  		<label for="fine_description" class="mg-label"><%= t('Description') %></label>
			                  	</td>
			                  	<td>
			                    	<%= f.text_area :fine_description ,:class=>"mg-text-field"%>
			                  	</td>
		                  	</tr>
		                  	<tr class="save-button-cls">
			                  	<td class="mg-p-mgtop" valign="top">
			                  		<%#= submit_tag 'Submit' ,:class=>"mg-small-form-btn"%>
			                  		<a onclick="$(this).closest('form').submit()" class="mg-small-form-btn"><%=t("save")%></a>
			                  		  <a class="mg-small-form-btn cancel-form-dialog"><%=t('cancel')%></a>

			                  	</td>
		                  	</tr>
              			</tbody>
              		</table>
              	</div>
              </div>
 		<% end %>
	</div>
</div>


 <script>
 	$(document).ready(function(){
 		$("#latefeeFineEditFormId").validate({
	      rules: 
	      {
	        "generate_fine_form[fine_name]": {required: true}
	      }
	    });
 	});
 </script>